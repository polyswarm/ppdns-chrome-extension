{
  "manifest_version": 3,
  "name": "PolySwarm in Browser",
  "description": "Earn NCT for contributing cyber security data",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "permissions": [
    "webNavigation",
    "webRequest",
    "declarativeNetRequest",
    "contextMenus",
    "downloads",
    "storage"
  ],
  "host_permissions": ["\u003Call_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [],
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png"],
      "matches": []
    }
  ]
}
