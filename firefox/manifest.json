{
  "description": "Earn NCT for contributing cyber security data",
  "version": "1.0.8",
  "manifest_version": 2,
  "name": "PolySwarm in Browser",
  "background": { "scripts": ["background.bundle.js"] },
  "permissions": ["webNavigation", "webRequest", "storage"],
  "host_permissions": ["<all_urls>"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": { "128": "icon-128.png", "38": "icon-34.png" },
  "content_scripts": [],
  "web_accessible_resources": ["icon-128.png", "icon-34.png"]
}
